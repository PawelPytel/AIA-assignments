@{
    var name = "Robert Paulson";
    if (Request["name"] != null && !Request["name"].IsEmpty())
    {
        name = Request["name"];
    }
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <ul>
        @for (int i = 0; i < 10; i++)
        {
            <li>@name</li>
        }
    </ul>
</body>
</html>
